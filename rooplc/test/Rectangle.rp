class Rectangle {
    width : int;
    height : int;

    init(width : int, height : int) : Rectangle {
        this->width = width;
        this->height = height;
        return this;
    }

    area() : int {
        return this->width * this->height;
    }

    perimeter() : int {
        return 2 * (this->width + this->height);
    }
}
